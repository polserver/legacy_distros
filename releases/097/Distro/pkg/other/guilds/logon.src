use os;

program Logon(who)

	var chr_guild := who.guild;
	var gm_serial := chr_guild.GetProp("Master");

	if ( CInt(who.serial) == CInt(gm_serial) )
		Start_Script(":guilds:requestAllyWar", who);
	endif

endprogram