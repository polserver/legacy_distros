include ":datafile:datafile";
include ":skilllocks:common";

program skillLock_OnDelete(character)
	var data_file  := SLPH_GetDataFile();
	if ( data_file )
		return data_file.DeleteElement(Hex(character.serial));
	endif
	return 1;
endprogram
