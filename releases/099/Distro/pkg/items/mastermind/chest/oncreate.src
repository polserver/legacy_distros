use uo;
use util;

include ":loot:lootParser";
include ":mastermind:mastermind";

program onCreate_MastermindChest( chest )

	chest.locked := 1;
 
        RandomSet( chest );

        var level := RandomInt(3)+3;
        
        Loot_Generate( chest, level, 1 );
        
        return 1;
endprogram
