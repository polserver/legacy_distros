use uo;

program OnDestroy( item )

	var SOSTile := SystemFindObjectBySerial( CInt( GetObjProperty( item, "SOSTileSerial" )));
        if( SOSTile )
                DestroyItem( SOSTile );
        endif
	
	return 1;
endprogram
		
