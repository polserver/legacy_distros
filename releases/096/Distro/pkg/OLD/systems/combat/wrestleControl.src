use uo;
use os;
use cfgfile;


program control_Wrestle(item)
	var cfgfile := ReadConfigFile(":combat:itemdesc");
	var dmg := cfgfile[item.objtype].damage;

	if(dmg)
		item.hitscript := "wrestlingHitScript";
	endif

	return;
endprogram
