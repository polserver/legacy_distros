use uo;
	
program command_GoSerial(who, text)
	var a := SystemFindObjectBySerial(CInt(text));
	if( !a )
		SendSysMessage(who,"No such thing.");
		return 0;
	endif
	
	MoveObjectToLocation(who, a.x, a.y, a.z, a.realm, MOVEOBJECT_FORCELOCATION);
	
	return 1;
endprogram
