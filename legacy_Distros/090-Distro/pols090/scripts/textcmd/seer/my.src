use uo;

program z(who, howfar)

local what := target(who, TGTOPT_NOCHECK_LOS);

if (!what)
        return;
endif

local tdist;

if (howfar = "")
        tdist := 1;
else
        tdist := cint(howfar);
endif


moveitemtolocation(what, what.x, what.y - tdist, what.z, MOVEITEM_FORCELOCATION );

endprogram