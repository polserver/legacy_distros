use http;

program blah( params )
    WriteHtml( "<HTML><HEAD><TITLE>Test Script-generated Page</TITLE></HEAD>" +
               "<BODY>This was generated by a script!" );

    WriteHtml( "a=" + QueryParam( "A" ) );
    WriteHtml( "b=" + QueryParam( "B" ) );

    WriteHtml( "</BODY></HTML>" );
endprogram