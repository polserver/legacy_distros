use uo;
use os;

program Help(who)
	var script := Start_Script(":help:helpSystem/helpSystem", who);
	if ( script.errortext )
		SendSysMessage(who, "Error starting help system ->"+script.errortext);
		return 0;
	endif

	return 1;
endprogram
