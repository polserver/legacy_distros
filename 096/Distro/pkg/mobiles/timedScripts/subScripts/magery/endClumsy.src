use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndClumsy(params)
	var mobile := params[1];

	var points := CInt(GetObjProperty(mobile, "#ClumsyMod"));	
	AP_ModifyStatMod(mobile, DEXTERITY, points);
	EraseObjProperty(mobile, "#ClumsyMod");

	return 1;
endprogram

