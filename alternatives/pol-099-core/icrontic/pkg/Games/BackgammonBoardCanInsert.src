use uo;

program BackgammonBoardCanInsert(who, container, item)
  var bbserial := CInt(GetObjProperty(item, "bbserial"));
  if (!bbserial)
    return 0;

  elseif (bbserial)
    if (bbserial != container.serial)
      return 0;

    elseif (bbserial = container.serial)
      return 1;
    endif
  endif
endprogram
